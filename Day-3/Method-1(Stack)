class Solution {
    public boolean isValid(String s) {
        Stack<Character> stack=new Stack<>();
        char ch[]=s.toCharArray();
        for(char i:ch){
            if(!stack.empty() && (stack.peek()=='(' && i==')' || stack.peek()=='{' && i=='}' || stack.peek()=='[' && i==']'))
                stack.pop();
            else stack.push(i);
        }
        if(stack.empty())
            return true;
        return false;
    }
}
